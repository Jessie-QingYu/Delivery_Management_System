<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="300.0" minWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.recipient.RecipientMainViewController">
   <children>
      <ImageView fitHeight="336.0" fitWidth="620.0" layoutX="1.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/recipient/RecipientMain.png" />
         </image>
      </ImageView>
      <Button fx:id="btnStatus" layoutX="329.0" layoutY="79.0" mnemonicParsing="false" onAction="#ClickStatus" text="Package Status" />
      <Button fx:id="btnConfirm" layoutX="329.0" layoutY="131.0" mnemonicParsing="false" onAction="#ClickComfirm" text="Confirm Receipt" />
      <Button fx:id="btnPreference" layoutX="329.0" layoutY="183.0" mnemonicParsing="false" onAction="#ClickPreference" text="Notification Preference" />
      <Button fx:id="btnRate" layoutX="329.0" layoutY="231.0" mnemonicParsing="false" onAction="#ClickRate" text="Rate Us" />
   </children>
</AnchorPane>
